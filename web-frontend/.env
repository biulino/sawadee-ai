# API Base URL Configuration
# Try localhost first, fallback to mock data if connection fails
REACT_APP_API_BASE_URL=http://localhost:8090/api

# Enable mock data fallback for development
REACT_APP_ENABLE_MOCK_FALLBACK=true

# VS Code Port Forwarding URLs (update these with your actual forwarded URLs)
# REACT_APP_API_BASE_URL=https://xxx-8090.preview.app.github.dev/api

# Disable service worker for development
GENERATE_SOURCEMAP=false
